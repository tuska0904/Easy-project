int startingNumberOfPoints = int.Parse(Console.ReadLine());
double points = 0.0;

if (startingNumberOfPoints <= 100)
{
    points = 5;
}
else if (startingNumberOfPoints < 1000)
{
    points = startingNumberOfPoints * 0.20;
}
else
{
    points = startingNumberOfPoints * 0.10;
}

if (startingNumberOfPoints % 2 == 0)
{
    points += 1;
}
else if (startingNumberOfPoints % 10 == 5)
{
    points += 2;
}

Console.WriteLine(points);
Console.WriteLine(points + startingNumberOfPoints);
